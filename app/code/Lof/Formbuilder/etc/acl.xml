<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magento_Backend::form_elements" title="Lof Formbuilder">
                    <resource id="Lof_Formbuilder::form" title="Form" sortOrder="10">
                        <resource id="Lof_Formbuilder::form_edit" title="Edit Form" sortOrder="10"/>
                        <resource id="Lof_Formbuilder::form_save" title="Save Form" sortOrder="20"/>
                        <resource id="Lof_Formbuilder::form_delete" title="Delete Form" sortOrder="30"/>
                    </resource>
                    <resource id="Lof_Formbuilder::message" title="Message" sortOrder="20">
                        <resource id="Lof_Formbuilder::message_delete" title="Delete Message" sortOrder="10"/>
                    </resource>
                    <resource id="Lof_Formbuilder::model" title="Model" sortOrder="30">
                        <resource id="Lof_Formbuilder::model_edit" title="Edit Model" sortOrder="10"/>
                        <resource id="Lof_Formbuilder::model_save" title="Save Model" sortOrder="20"/>
                        <resource id="Lof_Formbuilder::model_delete" title="Delete Model" sortOrder="30"/>
                    </resource>
                    <resource id="Lof_Formbuilder::category" title="Model Category" sortOrder="40">
                        <resource id="Lof_Formbuilder::category_edit" title="Edit Category" sortOrder="10"/>
                        <resource id="Lof_Formbuilder::category_save" title="Save Category" sortOrder="20"/>
                        <resource id="Lof_Formbuilder::category_delete" title="Delete Category" sortOrder="30"/>
                    </resource>
                </resource>
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="Lof_Formbuilder::config_form" title="Formbuilder"/>
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>